{
  "version": "2",
  "build_cookbook": {
    "name": "build",
    "path": ".delivery/build"
  },
  "delivery-truck": {
    "publish": {
      "github": "opscode-cookbooks/delivery-cluster"
    }
  },
  "skip_phases": [
    "security",
    "quality",
    "publish",
    "deploy",
    "smoke"
  ],
  "build_nodes": {
    "provision": ["name:builder-2.delivery.chef.co"],
    "functional": ["name:builder* NOT name:builder-2.delivery.chef.co"]
  }
}
